<div class="endorsements mx-auto mt-3 row single-row">
  <% secondary_sponsors.each do |e10t| %>
     <div class="e10t col-md mx-2 my-2 p-0 text-center">
       <a href="<%= e10t.link %>">
         <%= image_tag url_for(e10t.banner.variant(resize: "200x200")), alt: e10t.name, title: e10t.caption %>
       </a>
     </div>
     <% end %>
</div>
