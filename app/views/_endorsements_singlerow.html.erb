<div class="endorsements mx-5 row single-row justify-content-center">
  <% random_sponsors(4).each do |e10t| %>
     <div class="e10t col-sm mx-3 my-2 px-0">
       <a href="<%= e10t.link %>">
         <%= image_tag url_for(e10t.banner.variant(resize: "200x133")), alt: e10t.name, title: e10t.caption %>
       </a>
     </div>
     <% end %>
</div>
